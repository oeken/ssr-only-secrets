{
  "name": "ssr-only-secrets",
  "version": "0.1.0",
  "description": "This package provides a way to pass secrets from Server Components into the SSR-run of Client Components, without them being accessible in the browser.",
  "type": "module",
  "exports": {
    ".": {
      "react-server": {
        "default": "./dist/rsc.js"
      },
      "default": {
        "browser": "./dist/browser.js",
        "node": "./dist/ssr.js"
      }
    },
    "./package.json": "./package.json"
  },
  "repository": "https://github.com/phryneas/ssr-only-secrets",
  "author": "Lenz Weber-Tronic",
  "license": "MIT",
  "dependencies": {
    "optimism": "^0.17.5",
    "tsup": "^8.0.1"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.23.16",
    "@microsoft/api-extractor": "^7.39.1",
    "@tsconfig/recommended": "^1.0.3",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "typescript": "^5.3.3",
    "rimraf": "^5.0.5"
  },
  "peerDependencies": {
    "react": "^18"
  },
  "scripts": {
    "build": "rimraf dist; tsc; api-extractor run; api-documenter markdown --input-folder temp --output-folder docs"
  }
}
